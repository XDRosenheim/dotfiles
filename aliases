alias html='cd /var/www/html; l'
alias please='sudo $(history -p !-1)'
alias maze='while true; do (( RANDOM % 2 )) && echo -ne "\e[3$(( $RANDOM % 8 ))m/" || echo -n \\; sleep .02; done'
alias cd..='cd ..'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
alias ......='cd ../../../../..'
alias .......='cd ../../../../../..'
alias apt-get='sudo apt-get'
